      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>0.05 1.8 0.30</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.05 1.8 0.30</size>
            </box>
